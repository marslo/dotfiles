# how may days == ddiff YYYY-MM-DD now
hmdays() {
  usage="""USAGE:
  \n\t\$ hmdays YYYY-MM-DD
  \nExample:
  \n\t\$ hmdays 1987-03-08
  """

  if [ 1 -ne $# ]; then
    echo -e "${usage}"
  else
    if date +%s --date "$1" > /dev/null 2>&1; then
      echo $((($(date +%s)-$(date +%s --date "$1"))/(3600*24))) days
    else
      echo -e "${usage}"
    fi
  fi
}
