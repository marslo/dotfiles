" all files in pattern of **/jenkinsfile/**
if index(split( expand('%:p:h'), '/' ), 'jenkinsfile') >= 0
  let g:WebDevIconsUnicodeDecorateFileNodesPatternSymbols['.*']               = ''
endif

" 
" all files in pattern of **/.marslo/vimrc.d/**
