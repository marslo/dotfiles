# - colors:
#   - yellow: 178
#   - brightgreen: 192
#   - green: 2 (#98971a), 10 (#b8bb26) 106
#   - blue: 69, 75
#   - magenta: 135, 213, 104
#   - cyan: 81, 6 (#689d6a), 14(#8ec07c)
#   - grey: 241
# - attributes: bold, dim, ul, blink, reverse, italic, strike

[color]
  ui                = true
  log               = always
  status            = always
  add               = always
  push              = always
  pull              = always
  diff              = always
  branch            = always
  interactive       = always
[color "branch"]
  current           = 2 bold
  local             = 178
  remote            = 213 italic dim
[color "status"]
  added             = 192 italic
  updated           = 69 normal
  changed           = 64 normal
  untracked         = 135 italic
  unmerged          = 81 italic
  localBranch       = 178
  remoteBranch      = 213 dim
[color "diff"]
  meta              = 75 italic
  frag              = 104 italic
  old               = 241 bold italic strike
  # new             = 106
  new               = 2 bold
  whitespace        = 178 reverse
  commit            = 6 italic
[color "blame"]
  highlightRecent   = 239, 20 month ago, 240, 18 month ago, 241, 16 month ago, 242, 14 month ago, 243, 12 month ago, 244, 10 month ago, 245, 8 month ago, 246, 6 month ago, 247, 4 month ago, 131, 3 month ago, 137, 2 month ago, 172, 1 month ago, 167, 3 weeks ago, 166, 2 weeks ago, 203, 1 week ago, 202
  repeatedLines     = 130
[color "decorate"]
  remoteBranch      = 213 dim
  branch            = 178
  tag               = 100 italic
  HEAD              = 172

