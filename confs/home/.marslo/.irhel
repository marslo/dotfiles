GIT_COMPLETION_DIR="${iRCHOME}/utils/git-2.35.0/contrib/completion"
ls "${GIT_COMPLETION_DIR}"/*git* >/dev/null; [ $? -eq 0 ] && source "${GIT_COMPLETION_DIR}"/*git*

for _i in $( /bin/ls -1 ${iRCHOME}/.alias ); do
  source "${iRCHOME}/.alias/${_i}"
done

