# COL_DEFAULT="\[\033[2;38;5;${fgc}m\]"           # "\[$(tput setaf ${fgc})\]"; $"\e[38;5;${fgc}m";
COL_DEFAULT="\[\033[38;5;${fgc}m\]"               # "\[$(tput setaf ${fgc})\]"; $"\e[38;5;${fgc}m";
