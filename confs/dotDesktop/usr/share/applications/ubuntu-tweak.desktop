[Desktop Entry]
Name=Ubuntu Tweak
Comment=Tweak ubuntu to what you like
Exec=ubuntu-tweak
Icon=ubuntu-tweak
Terminal=false
Type=Application
Categories=GNOME;GTK;System;Settings;X-GNOME-Settings-Panel;X-GNOME-PersonalSettings;
X-GNOME-Settings-Panel=ubuntu-tweak
StartupNotify=true
X-Ayatana-Desktop-Shortcuts=Overview;Apps;Tweaks;Admins;Janitor
X-Ayatana-Appmenu-Show-Stubs=False
X-Ubuntu-Gettext-Domain=ubuntu-tweak

[Overview Shortcut Group]
Name=Overview
Exec=ubuntu-tweak -f overview
TargetEnvironment=Unity

[Apps Shortcut Group]
Name=Apps
Exec=ubuntu-tweak -f apps
TargetEnvironment=Unity

[Tweaks Shortcut Group]
Name=Tweaks
Exec=ubuntu-tweak -f tweaks
TargetEnvironment=Unity

[Admins Shortcut Group]
Name=Admins
Exec=ubuntu-tweak -f admins
TargetEnvironment=Unity

[Janitor Shortcut Group]
Name=Janitor
Exec=ubuntu-tweak -f janitor
TargetEnvironment=Unity
